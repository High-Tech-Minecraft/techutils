    "id": "${id}",
    "version": "${version}",
    "name": "${name}",
    "description": "",
    "authors": [
        "KikuGie"
    ],
    "contributors": [
        "VelizarBG"
    ],
    "contact": {
        "sources": "https://github.com/Kikugie/techutils",
        "issues": "https://github.com/Kikugie/techutils/issues"
    },
    "license": "Unlicense",
    "icon": "assets/techutils/icon.png",
